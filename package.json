    "@babel/core": "^7.6.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@tridnguyen/config": "^2.3.1",
    "@wdio/sauce-service": "^7.20.5",
    "@wdio/selenium-standalone-service": "^5.13.2",
    "allure-commandline": "^2.17.2",
