    "minimist": "^1.2.6",
    "mocha": "^10.2.0",
    "mocha-junit-reporter": "^2.0.2",
    "module-alias": "^2.1.0",
    "moment-timezone": "^0.5.26",
    "node-sass": "^4.9.0",
    "postcss-loader": "^3.0.0",
    "properties-parser": "^0.3.1",
    "proxyquire": "1.7.4",
    "request-promise": "^4.2.4",
    "sass": "^1.51.0",
    "sass-loader": "^7.3.1",
    "selenium-standalone": "^6.16.0",
